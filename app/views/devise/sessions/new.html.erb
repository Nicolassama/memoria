<div class="devise-sign-up">
  <h1 class="sign-up-headding">Log in</h2>

    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <div class="sign-up-main">

        <div class="field">
          <%= f.label :メールアドレス %><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "email-form" %>
        </div>

        <div class="field">
          <%= f.label :パスワード %><br />
          <%= f.password_field :password, autocomplete: "current-password", class: "email-form" %>
        </div>

          <%= f.submit "Log in", class: "actions" %><br>
    <% end %>

    <%= render "devise/shared/links" %>
</div>